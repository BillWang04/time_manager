{
    "manifest_version": 3,
    "name": "Time Tracker Extension",
    "version": "1.0",
    "description": "Tracks time spent on websites and compares to previous months",
    "permissions": [
        "webNavigation",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/startpage.html"
    }
}